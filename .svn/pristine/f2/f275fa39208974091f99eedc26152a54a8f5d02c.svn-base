from abc import ABCMeta, abstractproperty

class GenericParticleTransferProperties(object):
  __metaclass__ = ABCMeta

  def __init__(self):
    pass

  @abstractproperty
  def accelerator(self):
    pass
  
  @abstractproperty
  def particle_transfer(self):
    pass

  @abstractproperty
  def version(self):
    pass

  @abstractproperty
  def timing_device(self):
    pass
